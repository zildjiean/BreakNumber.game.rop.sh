#!/usr/bin/python3
import urllib
import re

response = urllib.urlopen('http://s30000-102007-vix.tarentum.hack.me/number.php') #change url when use hackme.
req = response.read()

def getvalues(x):
        line = str(x)
        text = re.search('(\-\d+|\d+), (\-\d+|\d+)\S+\s+\S+\s(\w+)',x)
        result = text.group(1,2,3)
        return result

print getvalues(req)

def calculate(result):
        x=int(result[0])
        if result[2] == 'maximum':
                for index in range(len(result)-1):
                        if x < int(result[index]):
                                x = result[index]
        else :
                for index in range(len(result)-1):
                        if x > int(result[index]):
                                x = result[index]
        return x

print calculate(getvalues(req))
